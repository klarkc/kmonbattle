{
  "name": "kmonbattle",
  "version": "1.0.0",
  "description": "Pokemon battle game for browsers",
  "main": "src/loader.js",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "babel-plugin-transform-es2015-classes": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "chai": "^3.5.0",
    "fakedom-amd": "github:klarkc/fakedom-amd.js#master",
    "http-server": "^0.9.0",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7"
  },
  "scripts": {
    "copy": "mkdir -p build && cp index.html build && cp -r bower_components build",
    "build": "npm run copy && babel src -d build/src --source-maps",
    "watch": "npm run copy && babel src -d build/src -w --source-maps",
    "start": "npm run watch & http-server build",
    "test": "mocha -u tdd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klarkc/kmonbattle.git"
  },
  "keywords": [
    "pokemon",
    "battle",
    "browser",
    "game"
  ],
  "author": "klarkc",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/klarkc/kmonbattle/issues"
  },
  "homepage": "https://github.com/klarkc/kmonbattle#readme",
  "dependencies": {}
}
